<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"   
						"http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap>
	<select id="getReport" resultClass="com.vos.Report">
		select * from tax_report
	</select>

	<insert id="insertReport" parameterClass="java.util.ArrayList">
		INSERT INTO
		tax_report(TAXID,IMPOSETYPE,YEAR,MONTH,PERIOD,STARTTIME,ENDTIME,DECLAREDATE,DEADLINE,DECLAREWAY,ENTRYDATE)
		VALUES
		<iterate conjunction=",">  
		(#value[].taxId#,#value[].imposeType#,#value[].year#,#value[].month#,#value[].period#,#value[].startTime#,#value[].endTime#,#value[].declareDate#,#value[].deadLine#,#value[].declareWay#,#value[].entryDate#)
		</iterate> 
	</insert>
	
</sqlMap>