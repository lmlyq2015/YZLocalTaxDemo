<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"   
						"http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap>
	<select id="getReport" resultClass="com.vos.Report">
		select * from tax_report
	</select>

	<insert id="insertReport" parameterClass="java.util.ArrayList">
		INSERT INTO
		tax_report(TAXID,IMPOSETYPE,YEAR,MONTH,PERIOD,STARTTIME,ENDTIME,DECLAREDATE,DEADLINE,DECLAREWAY,ENTRYDATE)
		VALUES
		<iterate conjunction=",">  
		(#value[].taxId#,#value[].imposeType#,#value[].year#,#value[].month#,#value[].period#,#value[].startTime#,#value[].endTime#,#value[].declareDate#,#value[].deadLine#,#value[].declareWay#,#value[].entryDate#)
		</iterate> 
	</insert>
	
	<insert id="insertComp" parameterClass="java.util.ArrayList">
		INSERT INTO
		tax_entbasicinfor(TAXID,TAXNAME,ADDRESS,REP,REPMOBILE,TAXADMIN,TAXAGENTNAME,TAXAGENTMOBILE,ADMINNAME,ADMINMOBILE,EID,STATE)
		VALUES
		<iterate conjunction=",">  
		(#value[].taxId#,#value[].taxName#,#value[].address#,#value[].rep#,#value[].repMobile#,#value[].taxAdmin#,#value[].taxAgentName#,#value[].taxAgentMobile#,#value[].adminName#,#value[].adminMobile#,#value[].eid#,#value[].state#)
		</iterate> 
	</insert>
	
	<select id="compareTaxId" resultClass="java.lang.Integer" parameterClass="java.util.Map">
		select count(*) from tax_entbasicinfor where TaxId = #compareTaxId#
	</select>
	
	<insert id="insertPay" parameterClass="java.util.ArrayList">
		INSERT INTO
		tax_pay(TAXID,IMPOSETYPE,PaymentDates,Deadline,TotaleTax,PaidTax,UnpaidTax,PushDate)
		VALUES
		<iterate conjunction=",">  
		(#value[].taxId#,#value[].imposeType#,#value[].paymentDates#,#value[].deadline#,#value[].totaleTax#,#value[].paidTax#,#value[].unpaidTax#,#value[].pushDate#)
		</iterate> 
	</insert>
	
	<select id="selectUnequalNum" resultClass="java.lang.Integer">
		select count(distinct(TaxId)) from tax_report where TaxId not in (select TaxId from tax_entbasicinfor)
	</select>
	
	<select id="selectUnequalTaxId" resultClass="com.vos.TaxId">
		select distinct(TaxId) from tax_report where TaxId not in (select TaxId from tax_entbasicinfor)
	</select>
	
	<select id="selectUnequalNumForpay" resultClass="java.lang.Integer">
		select count(distinct(TaxId)) from tax_pay where TaxId not in (select TaxId from tax_entbasicinfor)
	</select>
	
	<select id="selectUnequalTaxIdForpay" resultClass="com.vos.TaxId">
		select distinct(TaxId) from tax_pay where TaxId not in (select TaxId from tax_entbasicinfor)
	</select>
</sqlMap>