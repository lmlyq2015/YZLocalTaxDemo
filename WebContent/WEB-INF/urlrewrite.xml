<?xml version="1.0" encoding="utf-8"?>

<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.1//EN"

        "http://tuckey.org/res/dtds/urlrewrite3.1.dtd">

<urlrewrite>

       <rule>

              <from>/(.*).html</from>

              <to>/jsp/home/homePage.jsp</to>

       </rule>
		
	   <rule>
    		  <from>^/YZ/(\w+)/(\w+)</from>
              <to type="forward">/getContentByWebPage?mesId=$1&amp;taxId=$2</to>
       </rule>
</urlrewrite>
